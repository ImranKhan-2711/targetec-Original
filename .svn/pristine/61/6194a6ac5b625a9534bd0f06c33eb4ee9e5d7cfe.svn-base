package com.tec.model;

public class AccountDetail {

	private Long id;
	private Account account = null;
	private String firstName;
	private String lastName;
	private String email;
	private String phoneNumber;
	private ResourceMaster profileImage;

	public AccountDetail() {
		super();
	}

	public AccountDetail(Long id, Account account, String firstName, String lastName, String email, String phoneNumber,
			ResourceMaster profileImage) {
		super();
		this.id = id;
		this.account = account;
		this.firstName = firstName;
		this.lastName = lastName;
		this.email = email;
		this.phoneNumber = phoneNumber;
		this.profileImage = profileImage;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Account getAccount() {
		return account;
	}

	public void setAccount(Account account) {
		this.account = account;
	}

	public String getFirstName() {
		return firstName;
	}

	public void setFirstName(String firstName) {
		this.firstName = firstName;
	}

	public String getLastName() {
		return lastName;
	}

	public void setLastName(String lastName) {
		this.lastName = lastName;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getPhoneNumber() {
		return phoneNumber;
	}

	public void setPhoneNumber(String phoneNumber) {
		this.phoneNumber = phoneNumber;
	}

	public ResourceMaster getProfileImage() {
		return profileImage;
	}

	public void setProfileImage(ResourceMaster profileImage) {
		this.profileImage = profileImage;
	}

}
